#include <PRIZM.h>     
PRIZM prizm;

void setup() {
   prizm.PrizmBegin();
}

void loop() {
  prizm.setServoPosition(1,180);
  delay(500);
  prizm.setServoPosition(1,0);
  delay(500);
}

//Blink green LED
void GreenLED(){
  prizm.setGreedLED(HIGH);
  delay(250);
  prizm.setGreedLED(LOW);
  delay(250);
}

//Pulse Motor
void PulseMotor(){
 prizm.setMotorPower(1,50);
 delay(1000);
 prizm.setMotorPower(1,0);
 delay(1000);
}
